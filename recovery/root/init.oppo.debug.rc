    class main
    user root
    disabled
	oneshot	
service tranfer /system/bin/sh /autochmod.sh "tranfer"
    class main
    user root
    disabled
	oneshot
service tranfer_tomb /system/bin/sh /autochmod.sh "tranfer_tombstone"
    class main
